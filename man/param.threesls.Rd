\name{param.threesls}
\alias{param.threesls}
\title{Param Method for the \code{threesls} Zelig Model}
\usage{
  \method{param}{threesls}(obj, num, ...)
}
\arguments{
  \item{obj}{a 'zelig' object}

  \item{num}{an integer specifying the number of
  simulations to sample}

  \item{...}{ignored parameters}
}
\value{
  a list to be cast as a 'parameters' object
}
\description{
  Param Method for the \code{threesls} Zelig Model
}
\note{
  This method is used by the \code{threesls} Zelig model
}
\author{
  Matt Owen \email{mowen@iq.harvard.edu}
}

